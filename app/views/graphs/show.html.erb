<!-- Content Header (Page header) -->
<section class="content-header">
  <h1>Access Management - Graph View Editor<small>Version 1.0</small></h1>
  <ol class="breadcrumb">
	<li><a href="index.html"><i class="fa fa-connectdevelop"></i> Home</a></li>
	<li class="active">Graph View</li>
  </ol>
</section>

<!-- Main content -->
<section class="content">

	<div id="editNodeDialog" class="modal fade" role="dialog">
	  <div class="modal-dialog">

		<!-- Modal content-->
		<div class="modal-content">
		  <div class="modal-header">
			<button type="button" class="close" data-dismiss="modal">&times;</button>
			<h4 class="modal-title">Edit Node</h4>
		  </div>
		  <div class="modal-body">
			<div class="alert alert-danger" id="editNodeNameAlert" style="display: none"></div>
			<div class="form-group">
				<label for="shortNodeNameTxt">Short node name</label>
				<input class="form-control" type="text" maxlength="10" id="editShortNodeNameTxt" placeholder="Enter node name to be displayed here" required/>
			</div>
		  </div>
		  <div class="modal-footer">
			<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
			<button type="button" class="btn btn-primary" id="editNodeBtn">Save</button>
		  </div>
		</div>

	  </div>
	</div>

	<div class="box">
		<div class="box-body">
			<div class="font-lg">
				<div class="resource-legend pull-right form-group"><i class="fa fa-circle"></i> resources</div>
				<div class="user-legend pull-right form-group"><i class="fa fa-circle"></i> users</div>
				<div class="usergroup-legend pull-right form-group"><i class="fa fa-circle"></i> user groups</div>
			</div>
			<div id="graphDiv"></div>
		</div>
	</div>
</section>